
<%= form_for @user do |f|%>
	<%= f.label :name %>
	<%= f.text_field :name %>
<br>
	<%= f.label :user_name %>
	<%= f.text_field :user_name %>
	<br>
	<%= label_tag :address %>
  <%= f.text_field :address %>
<br>
	<%= f.label :city %>
  <%= f.text_field :city%>
<br>
	<%= f.label :state %>
  <%= f.text_field :state%>
<br>
	<%= f.label :zip%>
  <%= f.text_field :zip%>
<br>
  <%= f.submit %>
<% end %>
