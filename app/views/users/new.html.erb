<%= form_tag users_path do %>
<%= label_tag :user_name %>
<%= text_field_tag :user_name %>

<%= label_tag :password %>
<%= text_field_tag :password %>


  <%= label_tag :role %>
  <%= text_field_tag :role %>

  <%= label_tag :address %>
  <%= text_field_tag :address %>

  <%= label_tag :city %>
  <%= text_field_tag :city %>

  <%= label_tag :state %>
  <%= text_field_tag :state %>

  <%= label_tag :zip %>
  <%= text_field_tag :zip %>

  <%= submit_tag 'Create Profile' %>
<% end %>
